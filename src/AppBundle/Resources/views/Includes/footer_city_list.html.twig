<div class="container mt-3">
    <div class="row">
        <div class="col-12 text-center">
            <ul class="list-inline">
                {% for city in cityList %}
                <li class="list-inline-item">
                    <a href="https://{{ city.hostname }}/">
                        {{ city.title }}
                    </a>
                </li>
                {% endfor %}
            </ul>
        </div>
    </div>
</div>